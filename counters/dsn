    .thread T_0;
    .scope S_0000013b43d9bc20;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0000013b43d9b580_0;
    %inv;
    %store/vec4 v0000013b43d9b580_0, 0, 1;
