v0000011551186190_0 .net "q", 3 0, v00000115512a6a30_0;  1 drivers
v0000011551186230_0 .var "rst", 0 0;
S_00000115512ab090 .scope module, "DUT" "fbitsyncdw" 2 6, 3 1 0, S_00000115512aaf00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v00000115511860f0_0 .net "clk", 0 0, v00000115512abe50_0;  1 drivers
v00000115512a6a30_0 .var "count", 3 0;
v00000115512abdb0_0 .net "rst", 0 0, v0000011551186230_0;  1 drivers
E_00000115512ac340 .event posedge, v00000115512abdb0_0, v00000115511860f0_0;
    .scope S_00000115512ab090;
T_0 ;
    %wait E_00000115512ac340;
    %load/vec4 v00000115512abdb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v00000115512a6a30_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v00000115512a6a30_0;
    %subi 1, 0, 4;
    %assign/vec4 v00000115512a6a30_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00000115512aaf00;
T_1 ;
    %delay 5, 0;
    %load/vec4 v00000115512abe50_0;
    %inv;
    %store/vec4 v00000115512abe50_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00000115512aaf00;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000115512abe50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0000011551186230_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000011551186230_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_00000115512aaf00;
T_3 ;
    %vpi_call 2 14 "$dumpfile", "fbitsyncdwtb.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_00000115512aaf00 {0 0 0};
    %vpi_call 2 16 "$monitor", $time, " Clk=%b, Reset=%b Count=%b", v00000115512abe50_0, v0000011551186230_0, v0000011551186190_0 {0 0 0};
    %delay 300, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_3;
